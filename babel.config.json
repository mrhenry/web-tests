{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "bugfixes": true,
                "exclude": [
                    "@babel/plugin-transform-regenerator"
                ],
                "targets": {
                    "browsers": [
                        "IE >= 6",
                        "Opera >= 9",
                        "Safari >= 4",
                        "Chrome >= 4",
                        "Edge >= 12",
                        "Firefox >= 2"
                    ]
                },
                "useBuiltIns": false
            }
        ]
    ]
}
